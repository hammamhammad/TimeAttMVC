<div class="panel panel-default">
    <div class="panel-body-top">
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <h3 class="panel-title">@Resources.Resources.SearchOptionsTitle</h3>
                        <span class="pull-right flip clickable" id="pn_Excuseviewspan"><i class="glyphicon glyphicon-chevron-up"></i></span>
                    </div>
                    <div class="panel-body" id="pn_Excuseviewbody">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 form-inline">
                                <div class="form-group">
                                    <label for="Excuseview_FromDate" class="control-label">@Resources.Resources.FromLable</label>
                                    <div class="input-group dateinput">
                                        <input type="text" id="Excuseview_FromDate" class="form-control dpe" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.FromDateLable" readonly>
                                        <span class="input-group-addon add-on" onclick="$('#Excuseview_FromDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label for="Excuseview_ToDate" class="control-label">@Resources.Resources.ToLable</label>
                                    <div class="input-group dateinput">
                                        <input type="text" id="Excuseview_ToDate" class="form-control dpe" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.ToDateLable" readonly>
                                        <span class="input-group-addon add-on" onclick="$('#Excuseview_ToDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row top10-buffer">
                            <div class="col-md-12">
                                <div class="searchBy">
                                    <div class="col-md-3">
                                        <label for="txt_Select_ExcuseviewSection" class="control-label">@Resources.Resources.SectionLabe</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_ExcuseviewSection" class="form-control" placeholder="@Resources.Resources.SectionNameLable" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseviewSection:not(:disabled)').length == 0) { return }; Select_Section_Control = $('#txt_Select_ExcuseviewSection'); $('#SectionDialog').modal('show'); LoadSectionListTable()"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseviewSection:not(:disabled)').length == 0) { return }; $('#txt_Select_ExcuseviewSection').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="txt_Select_ExcuseviewBranch" class="control-label">@Resources.Resources.BranchLable</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_ExcuseviewBranch" class="form-control" placeholder="@Resources.Resources.BranchNameLable" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseviewBranch:not(:disabled)').length == 0) { return }; Select_Branch_Control = $('#txt_Select_ExcuseviewBranch'); $('#BranchDialog').modal('show'); LoadBranchListTable()"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseviewBranch:not(:disabled)').length == 0) { return }; $('#txt_Select_ExcuseviewBranch').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="txt_Select_ExcuseviewEmployee" class="control-label">@Resources.Resources.EmployeeLable</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_ExcuseviewEmployee" class="form-control" placeholder="@Resources.Resources.EmployeeNameLable" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseviewEmployee:not(:disabled)').length == 0) { return }; Select_Employee_Control = $('#txt_Select_ExcuseviewEmployee'); $('#EmployeeDialog').modal('show'); /*EmployeeListData = null;*/ LoadEmployeeListTable($('#txt_Select_ExcuseviewSection').attr('data-id'), $('#txt_Select_ExcuseviewBranch').attr('data-id'))"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseviewEmployee:not(:disabled)').length == 0) { return }; $('#txt_Select_ExcuseviewEmployee').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="txt_Select_ExcuseReason" class="control-label">@Resources.Resources.ExcuseReason</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_ExcuseReason" class="form-control" placeholder="@Resources.Resources.ExcuseReason" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseReason:not(:disabled)').length == 0) { return }; Select_ExcuseReason_Control = $('#txt_Select_ExcuseReason'); $('#ExcuseReasonListDialog').modal('show');"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcuseReason:not(:disabled)').length == 0) { return }; $('#txt_Select_ExcuseReason').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row col-centered">
                            <hr />
                            <button class="btn btn-success btn-lg" id="ExcuseviewbtSearch" onclick="SearchExcuses();">@Resources.Resources.btSearch </button>
                            <button class="btn btn-info btn-lg" id="PrintExcuse">@Resources.Resources.btPrint </button>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="row">

            <div class="col-md-12">
                <div class="pull-right flip padding10bottom">
                    <button class="btn btn-info btn-md" id="bt_AddExcuse">@Resources.Resources.bt_addnewExcuse</button>
                </div>
                <div class="clearfix"></div>
                <div class="panel panel-default">

                    <div class="panel-body-top fixed-panelWithScrollTimeSheet">

                        <div id="ExcusesviewTablediv">
                            <table id="ExcusesviewTable" class="table  table-bordered table-striped timesheet-table" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>@Resources.Resources.ClmEmpNo </th>
                                        <th>@Resources.Resources.ClmEmployeeName</th>
                                        <th>@Resources.Resources.ExcuseReason</th>
                                        <th>@Resources.Resources.ExcuseDateClm</th>
                                        <th>@Resources.Resources.TimeFromLable</th>
                                        <th>@Resources.Resources.TimeToLable</th>
                                        <th>@Resources.Resources.ClmtotalExecuse</th>
                                        <th>@Resources.Resources.ClmStatus</th>
                                        <th>@Resources.Resources.ClmEdit</th>
                                        <th>@Resources.Resources.ClmDelete</th>

                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="ExcuseFormDialog" class="modal fade" data-backdrop="static" role="dialog">

    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="pull-right flip">
                    <button type="button" class="btn btn-toolbar space2" aria-label="Close"><span class="glyphicon glyphicon-remove"></span></button>
                </div>
                <h4 class="modal-title" id="gridSystemModalExcuseTitle">@Resources.Resources.AddExcuseTitle </h4>
            </div>
            <div class="modal-body texr-start">
                <div class="row">
                    <form data-toggle="validator" class="form-horizontal" data-delay="0" role="form" id="frmExcuseFormDialog">
                        <div class="col-md-12 ">
                            <label for="txt_Select_ExcusesformEmployee" class="control-label col-xs-2 col-sm-3 col-md-2">@Resources.Resources.EmployeeNameLable </label>
                            <div class="form-group has-feedback">
                                <div class="col-xs-10 col-sm-9 col-md-10">
                                    <div class="input-group">
                                        <input type="text" id="txt_Select_ExcusesformEmployee" class="form-control" placeholder="@Resources.Resources.EmployeeNameLable" data-id="0" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                        <span class="input-group-addon add-on" onclick="if ($('#txt_Select_ExcusesformEmployee:not(:disabled)').length == 0) { return }; Select_Employee_Control = $('#txt_Select_ExcusesformEmployee'); $('#EmployeeDialog').modal('show'); LoadEmployeeListTable(0, 0)"><span class="glyphicon glyphicon-list-alt blue"></span></span>

                                    </div>
                                    <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                </div>

                            </div>
                        </div>
                        <div class="col-md-12 ">
                            <label for="ExcuseReason" class="control-label col-md-2"> @Resources.Resources.ExcuseTypeLable  </label>
                            <div class="form-group has-feedback">
                                <div class="col-md-9 ExcuseResonRadios columns">
                                    <label for="RB_ExcuseNormalReason" class="radio-inline">
                                        <input id="RB_ExcuseNormalReason" name="ExcuseReason" value="1" type="radio" onchange="ExcuseTypeRadioChanged(this);">
                                        @Resources.Resources.NormalLable
                                    </label>
                                    <label for="RB_ExcuseJobReason" class="radio-inline">
                                        <input id="RB_ExcuseJobReason" name="ExcuseReason" value="2" type="radio" onchange="ExcuseTypeRadioChanged(this);">
                                        @Resources.Resources.JobMissionLable
                                    </label>
                                    <label for="RB_RemotelyWorkReason" class="radio-inline">
                                        <input id="RB_RemotelyWorkReason" name="ExcuseReason" value="3" type="radio" onchange="ExcuseTypeRadioChanged(this);">
                                        @Resources.Resources.RemotelyWorkLable
                                    </label>
                                </div>
                            </div>

                        </div>
                        <div class="col-md-12 ">
                            <div class="form-group has-feedback">
                                <label for="Excuseform_FromDate" class="control-label col-md-2">@Resources.Resources.FromLable</label>
                                <div class="col-xs-10 col-sm-9 col-md-10">
                                    <div class="input-group ">
                                        <input type="text" id="Excuseform_FromDate" class="form-control dpef" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.FromLable" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                        <span class="input-group-addon add-on" onclick="$('#Excuseform_FromDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                    </div>
                                    <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                </div>
                            </div>
                            <div class="form-group has-feedback" id="divExcuseform_ToDate" style="display:none" >
                                <label for="Excuseform_ToDate" class="control-label col-md-2">@Resources.Resources.ToLable</label>
                                <div class="col-xs-10 col-sm-9 col-md-10">
                                    <div class="input-group ">
                                        <input type="text" id="Excuseform_ToDate" class="form-control dpef" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.ToLable" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                        <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                        <span class="input-group-addon add-on" onclick="$('#Excuseform_ToDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                    </div>
                                    <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12 ">
                            <label for="dd_ExcuseReason_edit" class="control-label col-md-2"> @Resources.Resources.ExcuseReason  </label>
                            <div class="form-group has-feedback">
                                <div class="col-md-9">
                                    <select class="form-control" id="dd_ExcuseReason_edit" required data-error="@Resources.Resources.FieldRequiredValidationMsg"></select>
                                    <span class="glyphicon form-control-feedback pull-right flip" aria-hidden="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 ">
                            <label for="shiftfromtimes" class="control-label col-md-2"> @Resources.Resources.ExcuseTime  </label>

                            <div class="col-md-10">
                                <div class="col-md-5 ">
                                    <div class=" form-group  has-feedback">

                                        <div>
                                            <input autocomplete="off" id="shiftfromtime" class="form-control" placeholder="@Resources.Resources.TimeFromLable" data-scroll-default="07:00" type="text" required data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                        </div>
                                        <span class="glyphicon form-control-feedback pull-right flip" aria-hidden="true"></span>
                                    </div>
                                </div>
                                <div class="col-md-5 ">
                                    <div class="form-group  has-feedback ">

                                        <div>
                                            <input autocomplete="off" id="shifttotime" class="form-control" data-scroll-default="16:00" placeholder="@Resources.Resources.TimeToLable" type="text" required data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                        </div>
                                        <span class="glyphicon form-control-feedback pull-right flip" aria-hidden="true"></span>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="col-md-12 ">
                            <label for="excNotes_edit" class="control-label col-md-2"> @Resources.Resources.NotesLable  </label>
                            <div class="form-group has-feedback">

                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="excNotes_edit" placeholder="@Resources.Resources.NotesLable">
                                </div>

                            </div>
                        </div>
                        <div class="col-md-12 ">
                            <label for="chk_IsApproved" class="control-label col-md-2">@Resources.Resources.ClmStatus</label>
                            <div class="form-group ">


                                <div class="col-xs-10 col-sm-9 checkbox" id="togglechk">
                                    <input data-toggle="toggle" data-on="@Resources.Resources.chk_Approved" data-width="150" data-off="@Resources.Resources.chk_NotApproved" type="checkbox" id="chk_IsApproved">
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-primary btn-lg" id="bt_saveExcuse"><span class="glyphicon glyphicon-ok"></span> @Resources.Resources.btSave</button>
                <button type="button" class="btn btn-warning btn-lg" id="bt_CancelExcuse" data-dismiss="modal">
                    <span class="glyphicon glyphicon-remove"></span>@Resources.Resources.btCancel
                </button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" role="dialog" id="ExcuseReasonListDialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="pull-right flip">
                    <button type="button" class="btn btn-toolbar space2" onclick="ReloadExcuseReasons(true); ReloadExcuseReasonList(false);" title="@Resources.Resources.btReload"><span class="glyphicon glyphicon-refresh"></span></button>

                    <button type="button" class="btn btn-toolbar space2" aria-label="Close"><span class="glyphicon glyphicon-remove"></span></button>
                </div>
                <h4 class="modal-title" id="gridSystemModalExcuseReasonListDialogLabel">@Resources.Resources.ExcuesReasonsTitle </h4>

            </div>
            <div class="modal-body">
                <div id="ExcuseReasoListTablediv">
                    <table id="ExcuseReasoListTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>@Resources.Resources.ExcuseReason</th>
                                <th>@Resources.Resources.ExcuseTypeLable</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>



