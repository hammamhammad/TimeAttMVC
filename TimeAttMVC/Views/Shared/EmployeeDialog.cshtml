<div class="modal fade" role="dialog" id="EmployeeDialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                
                    <div class="pull-right flip"    >
                        <button type="button" class="btn btn-toolbar space2" id="bt_referesh_empdata" onclick="ReloadEmployeeListTable(0, 0)" aria-label="Referesh" title="@Resources.Resources.btReload"><span class="glyphicon glyphicon-refresh"></span></button>
                        <button type="button" class="btn btn-toolbar space2"  aria-label="Close"><span class="glyphicon glyphicon-remove"></span></button>
                    </div>
                        <h4 class="modal-title" id="gridSystemModalEmpLabel">@Resources.Resources.EmployeeListTitle </h4>
            </div>
            <div class="modal-body">
                <div id="EmployeeListTablediv">
                    <table id="EmployeeListTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                
                                <th>@Resources.Resources.ClmEmpNo </th>
                                <th>@Resources.Resources.ClmEmployeeName</th>
                                <th>@Resources.Resources.ClmSectionName</th>
                                <th>@Resources.Resources.ClmBranchName</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>