<div class="modal fade" role="dialog" id="SectionDialog" aria-hidden="true" tabindex="-1">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="pull-right flip">
                    <button type="button" class="btn btn-toolbar space2" onclick="ReloadSectionListTable()" title="@Resources.Resources.btReload"><span class="glyphicon glyphicon-refresh"></span></button>

                    <button type="button" class="btn btn-toolbar space2"  aria-label="Close"><span class="glyphicon glyphicon-remove"></span></button>
                </div>
                <h4 class="modal-title" id="gridSystemModalSchLabel">@Resources.Resources.SectionListTitle </h4>

            </div>
            <div class="modal-body">
                <div id="SectionListTablediv">
                    <table id="SectionListTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>@Resources.Resources.ClmSectionNumber</th>
                                <th>@Resources.Resources.ClmSectionName</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
