<div class="panel panel-default">
    <div class="panel-body-top">
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <h3 class="panel-title">@Resources.Resources.SearchOptionsTitle</h3>
                        <span class="pull-right flip clickable" id="pn_Vacationsviewspan"><i class="glyphicon glyphicon-chevron-up"></i></span>
                    </div>
                    <div class="panel-body" id="pn_Vacationsviewbody">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 form-inline">
                                <div class="form-group">
                                    <label for="Vacationsview_FromDate" class="control-label">@Resources.Resources.FromLable</label>
                                    <div class="input-group dateinput">
                                        <input type="text" id="Vacationsview_FromDate" class="form-control dpv" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.FromDateLable" readonly>
                                        <span class="input-group-addon add-on" onclick="$('#Vacationsview_FromDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label for="Vacationsview_ToDate" class="control-label">@Resources.Resources.ToLable</label>
                                    <div class="input-group dateinput">
                                        <input type="text" id="Vacationsview_ToDate" class="form-control dpv" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.ToDateLable" readonly>
                                        <span class="input-group-addon add-on" onclick="$('#Vacationsview_ToDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row top10-buffer">
                            <div class="col-md-12">
                                <div class="searchBy">
                                    <div class="col-md-3">
                                        <label for="txt_Select_VacationsviewSection" class="control-label">@Resources.Resources.SectionLabe</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_VacationsviewSection" class="form-control" placeholder="@Resources.Resources.SectionNameLable" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewSection:not(:disabled)').length == 0) { return }; Select_Section_Control = $('#txt_Select_VacationsviewSection'); $('#SectionDialog').modal('show'); LoadSectionListTable()"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewSection:not(:disabled)').length == 0) { return }; $('#txt_Select_VacationsviewSection').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="txt_Select_VacationsviewBranch" class="control-label">@Resources.Resources.BranchLable</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_VacationsviewBranch" class="form-control" placeholder="@Resources.Resources.BranchNameLable" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewBranch:not(:disabled)').length == 0) { return }; Select_Branch_Control = $('#txt_Select_VacationsviewBranch'); $('#BranchDialog').modal('show'); LoadBranchListTable()"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewBranch:not(:disabled)').length == 0) { return }; $('#txt_Select_VacationsviewBranch').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="txt_Select_VacationsviewEmployee" class="control-label">@Resources.Resources.EmployeeLable</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_VacationsviewEmployee" class="form-control" placeholder="@Resources.Resources.EmployeeNameLable" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewEmployee:not(:disabled)').length == 0) { return }; Select_Employee_Control = $('#txt_Select_VacationsviewEmployee'); $('#EmployeeDialog').modal('show'); /*EmployeeListData = null;*/ LoadEmployeeListTable($('#txt_Select_VacationsviewSection').attr('data-id'), $('#txt_Select_VacationsviewBranch').attr('data-id'))"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewEmployee:not(:disabled)').length == 0) { return }; $('#txt_Select_VacationsviewEmployee').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="txt_Select_VacationsviewType" class="control-label">@Resources.Resources.VacationTypeClm</label>
                                        <div class="input-group">
                                            <input type="text" id="txt_Select_VacationsviewType" class="form-control" placeholder="@Resources.Resources.VacationTypeClm" data-id="0" readonly>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewType:not(:disabled)').length == 0) { return }; Select_VacType_Control = $('#txt_Select_VacationsviewType'); $('#VacationTypeListDialog').modal('show');"><span class="glyphicon glyphicon-list-alt blue"></span></span>
                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsviewType:not(:disabled)').length == 0) { return }; $('#txt_Select_VacationsviewType').attr('data-id', 0).val('').html('');"><span class="glyphicon glyphicon-remove red"></span></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row col-centered">
                            <hr />
                            <button class="btn btn-success btn-lg" id="VacationsviewbtSearch" onclick="SearchVacations();">@Resources.Resources.btSearch </button>
                            <button class="btn btn-info btn-lg" id="PrintVacation">@Resources.Resources.btPrint </button>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="row">

            <div class="col-md-12">
                <div class="pull-right flip padding10bottom">
                    <button class="btn btn-info btn-md" id="bt_AddVacation">@Resources.Resources.bt_addnewVacation</button>
                </div>
                <div class="clearfix"></div>
                <div class="panel panel-default">

                    <div class="panel-body-top fixed-panelWithScrollTimeSheet">

                        <div id="VacationsviewTablediv">
                            <table id="VacationsviewTable" class="table  table-bordered table-striped timesheet-table" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>@Resources.Resources.ClmEmpNo </th>
                                        <th>@Resources.Resources.ClmEmployeeName</th>
                                        <th>@Resources.Resources.VacationTypeClm</th>
                                        <th>@Resources.Resources.FromDateLable</th>
                                        <th>@Resources.Resources.ToDateLable</th>
                                        <th>@Resources.Resources.ClmStatus</th>
                                        <th>@Resources.Resources.ClmEdit</th>
                                        <th>@Resources.Resources.ClmDelete</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade unfold-3d" data-backdrop="static" role="dialog" id="VacationsDialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="pull-right flip">
                    <button type="button" class="btn btn-toolbar space2" aria-label="Close"><span class="glyphicon glyphicon-remove"></span></button>
                </div>
                <h4 class="modal-title" id="gridSystemModalvacationTitle">@Resources.Resources.VacationAddNewTitle </h4>
            </div>
            <div class="modal-body">
                <form class="form form-horizontal" data-delay="0" role="form" id="VacationForms">
                    <div class="row" id="NewDiv">
                        <div class="col-md-12">
                            <div class="panel panel-default">
                                <div class="panel-body">
                                    <div class="col-md-12">
                                        <div class="form-group VacationFormRadios">
                                            <div class="col-md-12">

                                                <label for="RB_ByEmployee" class="radio-inline">
                                                    <input id="RB_ByEmployee" name="VacationForm" value="divVacationFormbyemployee" type="radio" onchange="VacationFormRadioChanged(this);">
                                                    @Resources.Resources.VacationByEmployee
                                                </label>


                                                <label for="RB_ByBranch" class="radio-inline">
                                                    <input id="RB_ByBranch" name="VacationForm" value="divVacationFormbybranch" type="radio" onchange="VacationFormRadioChanged(this);">
                                                    @Resources.Resources.VacationByBranch
                                                </label>

                                                <label for="RB_BySection" class="radio-inline">
                                                    <input id="RB_BySection" name="VacationForm" value="divVacationFormbysection" type="radio" onchange="VacationFormRadioChanged(this);">
                                                    @Resources.Resources.VacationBySection
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-md-12 fixed-panel-60" style="display:none">

                                            <div id="divVacationFormbyemployee" class="VacationFormRadiosdiv" style="display:none">
                                                <div class="form-group has-feedback">
                                                    <label for="txt_Select_VacationsformEmployee" class="control-label col-xs-2 col-sm-3 col-md-2">@Resources.Resources.EmployeeNameLable </label>
                                                    <div class="col-xs-10 col-sm-9 col-md-10">
                                                        <div class="input-group">
                                                            <input type="text" id="txt_Select_VacationsformEmployee" class="form-control" placeholder="@Resources.Resources.EmployeeNameLable" data-id="0" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                                            <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsformEmployee:not(:disabled)').length == 0) { return }; Select_Employee_Control = $('#txt_Select_VacationsformEmployee'); $('#EmployeeDialog').modal('show'); LoadEmployeeListTable(0, 0)"><span class="glyphicon glyphicon-list-alt blue"></span></span>

                                                        </div>
                                                        <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                                    </div>

                                                </div>
                                            </div>

                                            <div id="divVacationFormbybranch" style="display:none" class="VacationFormRadiosdiv">
                                                <div class="form-group has-feedback">
                                                    <label for="txt_Select_ByBranch" class="control-label col-xs-2 col-sm-3 col-md-2">@Resources.Resources.BranchNameLable </label>
                                                    <div class="col-xs-10 col-sm-9 col-md-10">
                                                        <div class="input-group">
                                                            <input type="text" id="txt_Select_ByBranch" class="form-control" placeholder="@Resources.Resources.BranchNameLable" data-id="0" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                                            <span class="input-group-addon add-on" onclick="if (IsDisabled(this)) return; Select_Branch_Control = $('#txt_Select_ByBranch'); $('#BranchDialog').modal('show'); LoadBranchListTable()"><span class="glyphicon glyphicon-list-alt blue "></span></span>
                                                        </div>
                                                        <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                                    </div>

                                                </div>
                                            </div>
                                            <div id="divVacationFormbysection" style="display:none" class="VacationFormRadiosdiv">
                                                <div class="form-group has-feedback">
                                                    <label for="txt_Select_BySection" class="control-label col-xs-2 col-sm-3 col-md-2">@Resources.Resources.SectionNameLable </label>
                                                    <div class="col-xs-10 col-sm-9 col-md-10">
                                                        <div class="input-group">
                                                            <input type="text" id="txt_Select_BySection" class="form-control" placeholder="@Resources.Resources.SectionNameLable" data-id="0" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                                            <span class="input-group-addon add-on" onclick="if (IsDisabled(this)) return; Select_Section_Control = $('#txt_Select_BySection'); $('#SectionDialog').modal('show'); LoadSectionListTable()"><span class="glyphicon glyphicon-list-alt blue "></span></span>
                                                        </div>
                                                        <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row" id="EditDiv">
                        <div class="col-md-12">
                            <div class="panel panel-default">
                                <div class="panel-body texr-start">
                                    <div class="">
                                        <div class="col-md-5">
                                            <label for="txt_vac_employeno" class="control-label col-md-5">@Resources.Resources.EmployeeNoLable</label>
                                            <input type="text" id="txt_vac_employeno" readonly="readonly" class="form-control col-md-7" />
                                        </div>
                                        <div class="col-md-7">
                                            <label for="txt_vac_employename" class="control-label  col-md-3">@Resources.Resources.EmployeeNameLable</label>
                                            <input type="text" id="txt_vac_employename" readonly="readonly" class="form-control col-md-9" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="FormDiv" class="row">
                        <div class="col-md-12">
                            <div class="panel panel-default">
                                <div class="panel-body texr-start">


                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
                                        <div class="form-group has-feedback">
                                            <label for="Vacationsform_FromDate" class="control-label col-md-2">@Resources.Resources.FromLable</label>
                                            <div class="col-xs-10 col-sm-9 col-md-10">
                                                <div class="input-group ">
                                                    <input type="text" id="Vacationsform_FromDate" class="form-control dpvf" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.FromDateLable" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                                    <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                                    <span class="input-group-addon add-on" onclick="$('#Vacationsform_FromDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                                </div>
                                                <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                            </div>
                                        </div>
                                        <div class="form-group has-feedback">
                                            <label for="Vacationsform_ToDate" class="control-label col-md-2">@Resources.Resources.ToLable</label>
                                            <div class="col-xs-10 col-sm-9 col-md-10">
                                                <div class="input-group  ">
                                                    <input type="text" id="Vacationsform_ToDate" class="form-control dpvf" data-date-autoclose="true" data-date-format="yyyy-mm-dd" data-date-language="@Resources.Resources.lang" placeholder="@Resources.Resources.ToDateLable" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                                    <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                                    <span class="input-group-addon add-on" onclick="$('#Vacationsform_ToDate').datepicker('show');"><span class="glyphicon glyphicon-calendar"></span></span>
                                                </div>
                                                <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                            </div>
                                        </div>
                                        <div class="form-group has-feedback">

                                            <label for="txt_Select_VacationsformType" class="control-label col-md-2">@Resources.Resources.VacationTypeClm</label>
                                            <div class="col-xs-10 col-sm-9 col-md-10">
                                                <div class="input-group">
                                                    <input type="text" id="txt_Select_VacationsformType" class="form-control" placeholder="@Resources.Resources.VacationTypeClm" data-id="0" readonly data-required="true" data-error="@Resources.Resources.FieldRequiredValidationMsg">
                                                    <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                                                    <span class="input-group-addon add-on" onclick="if ($('#txt_Select_VacationsformType:not(:disabled)').length == 0) { return }; Select_VacType_Control = $('#txt_Select_VacationsformType'); $('#VacationTypeListDialog').modal('show');"><span class="glyphicon glyphicon-list-alt blue"></span></span>

                                                </div>
                                                <span class="help-block with-errors smallmargin-bottom pull-left flip"></span>
                                            </div>

                                        </div>
                                        <div class="form-group ">

                                            <label for="chk_IsApproved" class="control-label col-md-2">@Resources.Resources.ClmStatus</label>
                                            <div class="col-xs-10 col-sm-9 checkbox" id="togglechk">
                                                <input data-toggle="toggle" data-on="@Resources.Resources.chk_Approved" data-width="150" data-off="@Resources.Resources.chk_NotApproved" type="checkbox" id="chk_IsApproved">
                                            </div>
                                        </div>
                                    </div>


                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <div class="modal-footer">

                <div class="">
                    <button type="submit" class="btn btn-primary btn-lg" id="bt_saveVacationForm"><span class="glyphicon glyphicon-ok"></span> @Resources.Resources.btSave</button>
                    <button type="button" class="btn btn-warning btn-lg" id="bt_CancelVacationForm" data-dismiss="modal">
                        <span class="glyphicon glyphicon-remove"></span>@Resources.Resources.btCancel
                    </button>
                </div>
            </div>
        </div>



    </div>
</div>
<div class="modal fade" role="dialog" id="VacationTypeListDialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="pull-right flip">
                    <button type="button" class="btn btn-toolbar space2" onclick="ReloadVacationType(true); ReloadVacationTypeList(false);" title="@Resources.Resources.btReload"><span class="glyphicon glyphicon-refresh"></span></button>

                    <button type="button" class="btn btn-toolbar space2" aria-label="Close"><span class="glyphicon glyphicon-remove"></span></button>
                </div>
                <h4 class="modal-title" id="gridSystemModalVacationTypeListDialogLabel">@Resources.Resources.VacationsTypeTitle </h4>

            </div>
            <div class="modal-body">
                <div id="VacationTypeListTablediv">
                    <table id="VacationTypeListTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>@Resources.Resources.VacationTypeClm</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>